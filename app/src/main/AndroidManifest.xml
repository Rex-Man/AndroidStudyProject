<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oocl.manlimeng.androidstudyproject" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UIActivity.ViewStudyActivity"
            android:label="@string/title_activity_view_study" >
        </activity>
        <activity
            android:name=".UIActivity.SelfCreateActivity"
            android:label="@string/title_activity_self_create" >
        </activity>
        <activity
            android:name=".UIActivity.LinearLayoutActivity"
            android:label="@string/title_activity_linear_layout" >
        </activity>
        <activity
            android:name=".UIActivity.TableLayoutActivity"
            android:label="@string/title_activity_table_layout" >
        </activity>
    </application>

</manifest>
